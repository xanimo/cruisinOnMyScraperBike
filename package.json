{
  "name": "cruisinonmyscraperbike",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "server.js",
  "scripts": { 
    "build": "npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules public/scss/:public/css/",
    "css-lint": "stylelint public/scss/",
    "css-prefix": "postcss --replace public/css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "node server.js",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm-run-all css-lint css",
    "trace": "node --trace-deprecation server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xanimo/cruisinOnMyScraperBike.git"
  },
  "author": "xanimo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xanimo/cruisinOnMyScraperBike/issues"
  },
  "homepage": "https://github.com/xanimo/cruisinOnMyScraperBike#readme",
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^5.2.3",
    "dotenv": "^5.0.1",
    "es-module-shims": "^1.5.5",
    "express": "^4.16.3",
    "express-handlebars": "^7.0.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.7.5",
    "mongoose-pagination": "^1.0.0",
    "morgan": "^1.9.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.60.0",
    "stylelint": "^14.16.1",
    "stylelint-config-twbs-bootstrap": "^7.0.0",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  }
}
